{
  "name": "@railgun-community/cookbook",
  "version": "1.0.8",
  "license": "MIT",
  "main": "dist/index.js",
  "files": [
    "dist/**/*"
  ],
  "exports": {
    ".": "./dist/index.js"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm run clean && patch-package && tsc",
    "prepare": "npm run build",
    "check-circular-deps": "madge --circular .",
    "eslint": "eslint src --ext .ts,.tsx --fix",
    "lint": "npm run check-circular-deps && npm run eslint && tsc --noEmit && tsc --noEmit -p tsconfig.test.json",
    "test-fork": "env DEBUG=railgun*,ganache*,cookbook* NODE_ENV=test RUN_GANACHE_TESTS=1 npm test",
    "tsc-test": "tsc -p tsconfig.test.json && tsc-alias -p tsconfig.test.json",
    "compile-test": "npm run clean && npm run tsc-test",
    "test-coverage": "npm run compile-test && c8 mocha",
    "test": "npm run compile-test && NODE_ENV=test mocha"
  },
  "dependencies": {
    "@0x/contract-addresses": "^8.5.0",
    "@ethersproject/bignumber": "^5.7.0",
    "@ethersproject/providers": "^5.7.2",
    "@ethersproject/units": "^5.7.0",
    "@railgun-community/shared-models": "4.1.12",
    "axios": "^1.4.0",
    "ethers": "^5.7.2"
  },
  "devDependencies": {
    "@railgun-community/quickstart": "^4.3.23",
    "@types/chai": "^4.3.4",
    "@types/chai-as-promised": "^7.1.5",
    "@types/debug": "^4.1.7",
    "@types/leveldown": "^4.0.3",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.7.23",
    "@types/sinon": "^10.0.13",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "c8": "^7.12.0",
    "chai": "^4.3.7",
    "chai-as-promised": "^7.1.1",
    "debug": "^4.3.4",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.26.0",
    "ganache": "^7.8.0",
    "leveldown": "^6.1.1",
    "madge": "^5.0.1",
    "mocha": "^10.2.0",
    "patch-package": "^7.0.0",
    "prettier": "^2.8.3",
    "sinon": "^15.0.1",
    "snarkjs": "^0.6.10",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.2",
    "typescript": "^4.8.3"
  }
}
